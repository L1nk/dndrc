function abs() {
	echo "-- $1's Abilities --"
	cat $1 | grep '^\s*+'
}

function desc() {
	echo "-- $1's Description --"
	cat $1 | grep '\~'
}

function eq() {
	echo "-- $1's Equipment --"
	cat $1 | grep '\['
}

function stats() {
